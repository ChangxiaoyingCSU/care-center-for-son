<block wx:for="{{personalHousing}}" wx:key="index">
	<view  class="item-lsti-conent {{ index==2 ? 'sw1':''}}">
		<view class="data02-tupian">
			<view class="ysex01 {{item.zd=='1'?'':'hidden'}}"><text class="">优质服务</text></view>
			<image class="data02-img" src="{{item.img}}" alt="" />
		</view>
		<view class="data02-title">
			<view class="s1">{{item.title}}</view>
			<view class="s2">{{item.stmel}} {{item.stmels}}</view>

			<view class="s3">
				<span>{{item.specifications}}</span>
				<div class="vLine"></div>
				<span>好评率：</span>
				<span style="color:#384047">{{item.squaremeter}}%</span>
				<div class="vLine"></div>
				<span>{{item.homesuper}}</span>
			</view>

			<view class="s4">
				<text style="color:red;font-weight:800;">{{item.number}}</text>
				<text style="color:red;">K</text>
				<image style="width:40rpx;height:40rpx" src="{{item.avatar}}" />
				<text>{{item.type}}</text>
			</view>
		</view>
	</view>
</block>
